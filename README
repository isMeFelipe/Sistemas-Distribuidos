# COMANDOS INICIAIS
    # Instalar Dependencias
    composer install
    npm install

    # Gerar chave 
    php artisan key:generate

    # No BANCO (psql shell) rodar o comando:
    CREATE EXTENSION IF NOT EXISTS "uuid-ossp";

# Alterar .env
    # Tem que criar a database antes
    
    DB_CONNECTION=pgsql
    DB_HOST=localhost
    DB_PORT=5432
    DB_DATABASE=<nome_da_base>
    DB_USERNAME=<ususario>
    DB_PASSWORD=<senha>

# Iniciar banco
php artisan migrate

# Inicar Servidor # porta 8000 # porta 80001
php artisan serve 


# iniciar cliente
npm install

# rodar cliente
npm run dev


